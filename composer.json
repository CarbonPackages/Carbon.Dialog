{
    "name": "carbon/dialog",
    "type": "neos-carbon",
    "description": "This package provides a tiny Fusion helper for dialogues.",
    "license": "MIT",
    "keywords": [
        "flow",
        "neos",
        "fusion",
        "helper",
        "carbon"
    ],
    "require": {
        "neos/fusion": "^3.3 || ^4.0 || ^5.0 || 7.0",
        "neos/fusion-afx": "^1.0 || ^7.0",
        "carbon/eel": "^1.4 || ^2.0"
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Dialog\\": "Classes"
        }
    },
    "extra": {
        "installer-name": "Carbon.Dialog",
        "neos": {
            "package-key": "Carbon.Dialog"
        }
    }
}
