{
    "name": "carbon/dialog",
    "type": "neos-carbon",
    "description": "This package provides a tiny Fusion helper for dialogues.",
    "license": "GPL-3.0",
    "keywords": [
        "flow",
        "neos",
        "fusion",
        "helper",
        "carbon"
    ],
    "require": {
        "neos/fusion": "^5.3 || ^7.0 || ^8.0",
        "neos/fusion-afx": "^1.0 || ^7.0 || ^8.0",
        "carbon/eel": "^1.4 || ^2.0",
        "sitegeist/slipstream": "^1.1.1 || ^2.1.2"
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".github",
            ".editorconfig",
            ".eslintignore",
            ".eslintrc",
            ".gitattributes",
            ".gitignore",
            ".jshintrc",
            ".nvmrc",
            ".postcssrc.yml",
            ".prettierignore",
            ".prettierrc",
            ".stylelintrc",
            "CODE_OF_CONDUCT.md",
            "package.json",
            "pnpm-lock.yaml"
        ]
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Dialog\\": "Classes"
        }
    },
    "extra": {
        "installer-name": "Carbon.Dialog",
        "neos": {
            "package-key": "Carbon.Dialog"
        }
    }
}
